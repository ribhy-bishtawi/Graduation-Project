{
  "name": "snap-api-v1",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "migrate": "env $(cat .env) npx knex migrate:latest --knexfile src/config/knexfile.ts",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "prebuild": "swagger-cli bundle src/api/v1/docs/index.yaml --outfile src/api/v1/docs/index.json",
    "build": "rimraf dist && npx tsc && tsc-alias",
    "predev": "npm run build",
    "dev": "concurrently \"npx tsc -w\" \"tsc-alias -w\" \"nodemon dist/\"",
    "prestart": "npm run build",
    "start": "node dist/",
    "deploy": "claudia create --handler dist.src.handler.handler --deploy-proxy-api --region eu-central-1",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
